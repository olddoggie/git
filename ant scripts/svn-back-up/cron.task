#!/bin/sh
# This script is used for crontab using /bin/mv command
function cron_mv()
{
mv "$1" "$2"
}

case $1 in
  "mv_svnbacklog")
		echo "After cron job, moving log file to backup folder and recycle for next week..."
		cron_mv "/home/svn/back-up/svn2dump/svnback.log" "/home/svn/back-up/svn2dump/logs/svnback.until.`date +%Y-%m-%d`.log"
		echo "OK."
	;;
	*)
		echo "No such kind of ant scirpt .."
		exit 1
	;;
esac
